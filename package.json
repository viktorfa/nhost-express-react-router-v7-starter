{
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "cross-env NODE_ENV=development node server.js",
    "start": "node server.js",
    "typecheck": "react-router typegen && tsc -b",
    "generate-types": "concurrently \"npm run generate-gql-types\" \"npm run generate-kysely-types\"",
    "generate-gql-types": "graphql-codegen --config graphql-codegen.ts",
    "generate-kysely-types": "kysely-codegen --out-file src/generated/kysely.d.ts --exclude-pattern hdb_* --env-file .env.local",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "nhost": "NHOST_PROJECT_NAME=tokens nhost",
    "pghero": "docker run -ti -e DATABASE_URL=$DATABASE_URL -p 8080:8080 ankane/pghero",
    "dexter": "dexter $DATABASE_URL --pg-stat-statements",
    "metadata": "hasura metadata --project nhost",
    "migrate": "hasura migrate --project nhost --database-name default",
    "seed": "hasura seed --project nhost --database-name default",
    "pgadmin": "docker run --network tokens_default --name pgadmin -p 5050:5050 -e PGADMIN_DEFAULT_EMAIL=pgadmin@pgadmin.org -e PGADMIN_DEFAULT_PASSWORD=pgadmin -e PGADMIN_LISTEN_PORT=5050 -e PGADMIN_CONFIG_SERVER_MODE=False --link postgres --rm dpage/pgadmin4"
  },
  "dependencies": {
    "@apollo/client": "^3.12.9",
    "@hookform/resolvers": "^3.10.0",
    "@nhost/nhost-js": "^3.2.4",
    "@nhost/react": "^3.9.1",
    "@nhost/react-apollo": "^16.0.1",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@react-router/express": "^7.1.5",
    "@react-router/node": "^7.1.5",
    "@tanstack/react-query": "^5.66.0",
    "change-case-all": "^1.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.5",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.3",
    "isbot": "^5.1.22",
    "kysely": "^0.27.5",
    "lucide-react": "^0.474.0",
    "morgan": "^1.10.0",
    "nuqs": "^2.3.2",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-operations": "^4.4.1",
    "@react-router/dev": "^7.1.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.17",
    "@types/pg": "^8.11.11",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^9.1.0",
    "kysely-codegen": "^0.17.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.6.1"
  }
}